ial\")])])])},function(){var a=this,t=a._self._c;return t(\"div\",{staticClass:\"col-lg-10 col-xl-6 mx-auto mb-5\"},[t(\"div\",{staticClass:\"titulo-sexto color-acento-contenido mb-3\"},[t(\"h5\",[a._v(\"Tabla 24.\")]),t(\"span\",[a._v(\"Primer registro: capital autorizado y por suscribir\")])]),t(\"div\",{staticClass:\"tabla-a mb-4\",attrs:{\"data-aos\":\"fade-up\"}},[t(\"table\",{staticClass:\"table-type-1\"},[t(\"thead\",[t(\"tr\",[t(\"th\",[a._v(\"CUENTA\")]),t(\"th\",[a._v(\"DÉBITOS\")]),t(\"th\",[a._v(\"CRÉDITOS\")])])]),t(\"tbody\",[t(\"tr\",{staticClass:\"bg-white\"},[t(\"td\",{staticClass:\"fw-bold text-start\"},[a._v(\"Capital por suscribir\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"$ 300.000.000\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"-\")])]),t(\"tr\",{staticClass:\"bg-white\"},[t(\"td\",{staticClass:\"fw-bold text-start\"},[a._v(\"Capital autorizado\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"-\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"$ 300.000.000\")])])])])]),t(\"div\",{staticClass:\"titulo-sexto color-acento-contenido mb-3\"},[t(\"h5\",[a._v(\"Tabla 25.\")]),t(\"span\",[a._v(\"Segundo registro: capital suscrito y pagado parcialmente\")])]),t(\"div\",{staticClass:\"tabla-a mb-4\",attrs:{\"data-aos\":\"fade-up\"}},[t(\"table\",{staticClass:\"table-type-1\"},[t(\"thead\",[t(\"tr\",[t(\"th\",[a._v(\"CUENTA\")]),t(\"th\",[a._v(\"DÉBITOS\")]),t(\"th\",[a._v(\"CRÉDITOS\")])])]),t(\"tbody\",[t(\"tr\",{staticClass:\"bg-white\"},[t(\"td\",{staticClass:\"fw-bold text-start\"},[a._v(\"Efectivo y sus equivalentes\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"$ 105.000.000\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"-\")])]),t(\"tr\",{staticClass:\"bg-white\"},[t(\"td\",{staticClass:\"fw-bold text-start\"},[a._v(\"Capital suscrito por pagar\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"$ 45.000.000\")]),t(\"td\",{staticClass:\"texto-right\"},[a._v(\"-\")])]),t(\"tr\",{staticClass:\"bg-white\"},[t(\"td\",{